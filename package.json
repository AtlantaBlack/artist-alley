{
  "name": "artist-alley",
  "version": "1.0.0",
  "description": "A place for artists to showcase and sell their work",
  "main": "server/server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
    "develop": "concurrently \"cd server && npm run watch\" \"cd client && npm start\"",
    "install": "cd server && npm i && cd ../client && npm i",
    "seed": "cd server && npm run seed",
    "build": "cd client && npm run build",
    "prepare": "husky install",
    "lint": "npx eslint \"**/*.js\"",
    "prettier-check": "npx prettier --check .",
    "prettier-write": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AtlantaBlack/artist-alley.git"
  },
  "keywords": [
    "artist",
    "alley"
  ],
  "author": "Sushan Yue, Christi Scappatura",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AtlantaBlack/artist-alley/issues"
  },
  "homepage": "https://github.com/AtlantaBlack/artist-alley#readme",
  "dependencies": {
    "concurrently": "^7.3.0",
    "husky": "^8.0.1"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.7.1",
    "husky": "^8.0.0"
  }
}
